[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Form;[m
[32m+[m
[32m+[m[32muse App\Entity\Ad;[m
[32m+[m[32muse App\Form\ImageType;[m
[32m+[m[32muse Symfony\Component\Form\AbstractType;[m
[32m+[m[32muse Symfony\Component\Form\FormBuilderInterface;[m
[32m+[m[32muse Symfony\Component\OptionsResolver\OptionsResolver;[m
[32m+[m[32muse Symfony\Component\Form\Extension\Core\Type\UrlType;[m
[32m+[m[32muse Symfony\Component\Form\Extension\Core\Type\TextType;[m
[32m+[m[32muse Symfony\Component\Form\Extension\Core\Type\MoneyType;[m
[32m+[m[32muse Symfony\Component\Form\Extension\Core\Type\IntegerType;[m
[32m+[m[32muse Symfony\Component\Form\Extension\Core\Type\TextareaType;[m
[32m+[m[32muse Symfony\Component\Form\Extension\Core\Type\CollectionType;[m
[32m+[m
[32m+[m[32mclass AdType extends AbstractType[m
[32m+[m
[32m+[m[32m{[m
[32m+[m[32m    /**[m
[32m+[m[32m     *[m[41m [m
[32m+[m[32m     * Permet d'avoir la configuration de base d'un champ ![m
[32m+[m[32m     *[m
[32m+[m[32m     * @param string $label[m
[32m+[m[32m     * @param string $placeholder[m
[32m+[m[32m     * @param array $options[m
[32m+[m[32m     * @return array[m
[32m+[m[32m     */[m
[32m+[m[32m    private function getConfiguration($label, $placeholder, $options = [] ){[m
[32m+[m[32m        return array_merge( [[m
[32m+[m[32m            'label' => $label,[m
[32m+[m[32m            'attr' =>[[m
[32m+[m[32m                'placeholder' =>$placeholder[m
[32m+[m[32m            ][m
[32m+[m[32m        ], $options);[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    public function buildForm(FormBuilderInterface $builder, array $options)[m
[32m+[m[32m    {[m
[32m+[m[32m        $builder[m
[32m+[m[32m            ->add('title',TextType::class,$this->getConfiguration("Titre","Tapez un titre pour votre annonce"))[m
[32m+[m[41m               [m
[32m+[m[41m            [m
[32m+[m[32m            ->add('slug',TextType::class, $this->getConfiguration("Adresse web","Tapez[m[41m [m
[32m+[m[32m            ladresse web(automatique)",[[m
[32m+[m[32m                'required'=> false[m
[32m+[m[32m            ]))[m
[32m+[m[32m            ->add('coverImage', UrlType::class, $this->getConfiguration("URL de l'image principale", " Donnez l'adresse d'une image"))[m
[32m+[m[32m            ->add('introduction', TextType::class, $this->getConfiguration("Introduction","Donnez une description globale de l'annonce"))[m
[32m+[m[32m            ->add('content', TextareaType::class, $this->getConfiguration("Description d√©taill√©e", "Tapez une description"))[m
[32m+[m[32m            ->add('rooms', IntegerType::class,$this->getConfiguration("Nombre de chambres", " Le nombre de chambre"))[m
[32m+[m[32m            ->add('price', MoneyType::class, $this->getConfiguration("Prix par nuit"," Indiquez le prix que vous voulez pour une nuit"))[m
[32m+[m[41m            [m
[32m+[m[32m            ->add([m
[32m+[m[32m                'images',[m
[32m+[m[32m                CollectionType::class,[m
[32m+[m[32m                [[m
[32m+[m[32m                    'entry_type' => ImageType::class,[m
[32m+[m[32m                    'allow_add' => true,[m
[32m+[m[32m                    'allow_delete'=> true[m
[32m+[m[32m                ][m
[32m+[m[32m                )[m
[32m+[m[41m            [m
[32m+[m[32m        ;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function configureOptions(OptionsResolver $resolver)[m
[32m+[m[32m    {[m
[32m+[m[32m        $resolver->setDefaults([[m
[32m+[m[32m            'data_class' => Ad::class,[m
[32m+[m[32m        ]);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
