[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * @ORM\Entity(repositoryClass="App\Repository\ImageRepository")[m
[32m+[m[32m */[m
[32m+[m[32mclass Image[m
[32m+[m[32m{[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @ORM\Id()[m
[32m+[m[32m     * @ORM\GeneratedValue()[m
[32m+[m[32m     * @ORM\Column(type="integer")[m
[32m+[m[32m     */[m
[32m+[m[32m    private $id;[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @ORM\Column(type="string", length=255)[m
[32m+[m[32m     * @Assert\Url()[m
[32m+[m[32m     */[m
[32m+[m[32m    private $url;[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @ORM\Column(type="string", length=255)[m
[32m+[m[32m     * @Assert\Length(min=10,minMessage="Le titre de l'image doit faire au moins 10 caract√®res")[m
[32m+[m[32m     */[m
[32m+[m[32m    private $caption;[m
[32m+[m
