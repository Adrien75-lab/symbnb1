[32m+[m[32m# This file is the entry point to configure your own services.[m
[32m+[m[32m# Files in the packages/ subdirectory configure your dependencies.[m
[32m+[m
[32m+[m[32m# Put parameters here that don't need to change on each machine where the app is deployed[m
[32m+[m[32m# https://symfony.com/doc/current/best_practices/configuration.html#application-related-configuration[m
[32m+[m[32mparameters:[m
[32m+[m
[32m+[m[32mservices:[m
[32m+[m[32m    # default configuration for services in *this* file[m
[32m+[m[32m    _defaults:[m
[32m+[m[32m        autowire: true      # Automatically injects dependencies in your services.[m
[32m+[m[32m        autoconfigure: true # Automatically registers your services as commands, event subscribers, etc.[m
[32m+[m
[32m+[m[32m    # makes classes in src/ available to be used as services[m
[32m+[m[32m    # this creates a service per class whose id is the fully-qualified class name[m
[32m+[m[32m    App\:[m
[32m+[m[32m        resource: '../src/*'[m
[32m+[m[32m        exclude: '../src/{DependencyInjection,Entity,Migrations,Tests,Kernel.php}'[m
[32m+[m
[32m+[m[32m    # controllers are imported separately to make sure services can be injected[m
[32m+[m[32m    # as action arguments even if you don't extend any base controller class[m
[32m+[m[32m    App\Controller\:[m
[32m+[m[32m        resource: '../src/Controller'[m
[32m+[m[32m        tags: ['controller.service_arguments'][m
[32m+[m
[32m+[m[32m    # add more service definitions when explicit configuration is needed[m
[32m+[m[32m    # please note that last definitions always *replace* previous ones[m
