[1m+++ b/src/Kernel.php[m
[36m@@ -0,0 +1,53 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App;[m
[32m+[m
[32m+[m[32muse Symfony\Bundle\FrameworkBundle\Kernel\MicroKernelTrait;[m
[32m+[m[32muse Symfony\Component\Config\Loader\LoaderInterface;[m
[32m+[m[32muse Symfony\Component\Config\Resource\FileResource;[m
[32m+[m[32muse Symfony\Component\DependencyInjection\ContainerBuilder;[m
[32m+[m[32muse Symfony\Component\HttpKernel\Kernel as BaseKernel;[m
[32m+[m[32muse Symfony\Component\Routing\RouteCollectionBuilder;[m
[32m+[m
[32m+[m[32mclass Kernel extends BaseKernel[m
[32m+[m[32m{[m
[32m+[m[32m    use MicroKernelTrait;[m
[32m+[m
[32m+[m[32m    private const CONFIG_EXTS = '.{php,xml,yaml,yml}';[m
[32m+[m
[32m+[m[32m    public function registerBundles(): iterable[m
[32m+[m[32m    {[m
[32m+[m[32m        $contents = require $this->getProjectDir().'/config/bundles.php';[m
[32m+[m[32m        foreach ($contents as $class => $envs) {[m
[32m+[m[32m            if ($envs[$this->environment] ?? $envs['all'] ?? false) {[m
[32m+[m[32m                yield new $class();[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function getProjectDir(): string[m
[32m+[m[32m    {[m
[32m+[m[32m        return \dirname(__DIR__);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    protected function configureContainer(ContainerBuilder $container, LoaderInterface $loader): void[m
[32m+[m[32m    {[m
[32m+[m[32m        $container->addResource(new FileResource($this->getProjectDir().'/config/bundles.php'));[m
[32m+[m[32m        $container->setParameter('container.dumper.inline_class_loader', true);[m
[32m+[m[32m        $confDir = $this->getProjectDir().'/config';[m
[32m+[m
[32m+[m[32m        $loader->load($confDir.'/{packages}/*'.self::CONFIG_EXTS, 'glob');[m
[32m+[m[32m        $loader->load($confDir.'/{packages}/'.$this->environment.'/**/*'.self::CONFIG_EXTS, 'glob');[m
[32m+[m[32m        $loader->load($confDir.'/{services}'.self::CONFIG_EXTS, 'glob');[m
[32m+[m[32m        $loader->load($confDir.'/{services}_'.$this->environment.self::CONFIG_EXTS, 'glob');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    protected function configureRoutes(RouteCollectionBuilder $routes): void[m
[32m+[m[32m    {[m
[32m+[m[32m        $confDir = $this->getProjectDir().'/config';[m
[32m+[m
[32m+[m[32m        $routes->import($confDir.'/{routes}/'.$this->environment.'/**/*'.self::CONFIG_EXTS, '/', 'glob');[m
[32m+[m[32m        $routes->import($confDir.'/{routes}/*'.self::CONFIG_EXTS, '/', 'glob');[m
[32m+[m[32m        $routes->import($confDir.'/{routes}'.self::CONFIG_EXTS, '/', 'glob');[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
