[32m+[m[32m    public function addAd(Ad $ad): self[m
[32m+[m[32m    {[m
[32m+[m[32m        if (!$this->ads->contains($ad)) {[m
[32m+[m[32m            $this->ads[] = $ad;[m
[32m+[m[32m            $ad->setAuthor($this);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $this;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function removeAd(Ad $ad): self[m
[32m+[m[32m    {[m
[32m+[m[32m        if ($this->ads->contains($ad)) {[m
[32m+[m[32m            $this->ads->removeElement($ad);[m
[32m+[m[32m            // set the owning side to null (unless already changed)[m
[32m+[m[32m            if ($ad->getAuthor() === $this) {[m
[32m+[m[32m                $ad->setAuthor(null);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $this;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function getRoles(){[m
[32m+[m[32m        return ['ROLE_USER'];[m
[32m+[m
[32m+[m[32m    }[m
