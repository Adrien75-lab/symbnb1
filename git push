[32m+[m
[32m+[m[32mclass PasswordUpdateType extends ApplicationType[m
[32m+[m[32m{[m
[32m+[m[32m    public function buildForm(FormBuilderInterface $builder, array $options)[m
[32m+[m[32m    {[m
[32m+[m[32m        $builder[m
[32m+[m[32m            ->add('oldPassword', PasswordType::class, $this->getConfiguration("Ancien mot de passe",[m
