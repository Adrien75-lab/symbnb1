[32m+[m[32m     * @ORM\JoinColumn(nullable=false)[m
[32m+[m[32m     */[m
[32m+[m[32m    private $ad;[m
[32m+[m
[32m+[m[32m    public function getId(): ?int[m
[32m+[m[32m    {[m
[32m+[m[32m        return $this->id;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function getUrl(): ?string[m
