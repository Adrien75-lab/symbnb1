[32m+[m
[32m+[m[32m    public function register(Request $request, ObjectManager $manager, UserPasswordEncoderInterface $encoder ){[m
[32m+[m
[32m+[m[32m        $user = new User();[m
[32m+[m[32m        $form = $this->createForm(RegistrationType::class, $user);[m
[32m+[m[32m        $form->handleRequest($request);[m
[32m+[m
[32m+[m[32m        if($form->isSubmitted() && $form->isValid()){[m
[32m+[m[32m            $hash = $encoder->encodePassword($user,$user->getHash());[m
[32m+[m[32m            $user->setHash($hash);[m
[32m+[m
[32m+[m[32m            $manager->persist($user);[m
[32m+[m[32m            $manager->flush();[m
[32m+[m
[32m+[m[32m            $this->addFlash([m
[32m+[m[32m             'sucess',[m
[32m+[m[32m             "Votre compte a bien √©t√© cr√©e! Vous pouvez maintenant vous connecter"[m
[32m+[m
[32m+[m[32m            );[m
[32m+[m[32m            return $this->redirectToRoute("account_login");[m
[32m+[m[32m        }[m
[32m+[m[32m        return $this->render('account/registration.html.twig', [[m
[32m+[m[32m            'form' => $form->createView()[m
[32m+[m[32m            ]);[m
[32m+[m[32m        }[m
