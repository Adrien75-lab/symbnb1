[32m+[m[32m#!/usr/bin/env php[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32muse App\Kernel;[m
[32m+[m[32muse Symfony\Bundle\FrameworkBundle\Console\Application;[m
[32m+[m[32muse Symfony\Component\Console\Input\ArgvInput;[m
[32m+[m[32muse Symfony\Component\Debug\Debug;[m
[32m+[m
[32m+[m[32mif (false === in_array(\PHP_SAPI, ['cli', 'phpdbg', 'embed'], true)) {[m
[32m+[m[32m    echo 'Warning: The console should be invoked via the CLI version of PHP, not the '.\PHP_SAPI.' SAPI'.\PHP_EOL;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mset_time_limit(0);[m
[32m+[m
[32m+[m[32mrequire dirname(__DIR__).'/vendor/autoload.php';[m
[32m+[m
[32m+[m[32mif (!class_exists(Application::class)) {[m
[32m+[m[32m    throw new RuntimeException('You need to add "symfony/framework-bundle" as a Composer dependency.');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m$input = new ArgvInput();[m
[32m+[m[32mif (null !== $env = $input->getParameterOption(['--env', '-e'], null, true)) {[m
[32m+[m[32m    putenv('APP_ENV='.$_SERVER['APP_ENV'] = $_ENV['APP_ENV'] = $env);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mif ($input->hasParameterOption('--no-debug', true)) {[m
[32m+[m[32m    putenv('APP_DEBUG='.$_SERVER['APP_DEBUG'] = $_ENV['APP_DEBUG'] = '0');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mrequire dirname(__DIR__).'/config/bootstrap.php';[m
[32m+[m
[32m+[m[32mif ($_SERVER['APP_DEBUG']) {[m
[32m+[m[32m    umask(0000);[m
[32m+[m
[32m+[m[32m    if (class_exists(Debug::class)) {[m
[32m+[m[32m        Debug::enable();[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m$kernel = new Kernel($_SERVER['APP_ENV'], (bool) $_SERVER['APP_DEBUG']);[m
[32m+[m[32m$application = new Application($kernel);[m
[32m+[m[32m$application->run($input);[m
