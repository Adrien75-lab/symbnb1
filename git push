[32m+[m[32m         * @return Response[m
[32m+[m[32m         */[m
[32m+[m
[32m+[m[32m        public function updatePassword(Request $request, UserPasswordEncoderInterface $encoder, ObjectManager $manager){[m
[32m+[m[32m            $passwordUpdate = new PasswordUpdate();[m
[32m+[m[32m            $user = $this->getUser();[m
[32m+[m[32m            $form = $this->createForm(PasswordUpdateType::class, $passwordUpdate);[m
[32m+[m[32m            $form->handleRequest($request);[m
[32m+[m[32m            if($form->isSubmitted()&& $form->isValid()){[m
[32m+[m[32m                // 1. Verifier que le oldPassword du formulaire soit le meme que le password de l'user[m
[32m+[m[32m                if (!password_verify($passwordUpdate->getOldPassword(),$user->getHash())){[m
[32m+[m[41m                    [m
[32m+[m[32m                // G√©rer l'erreur[m
[32m+[m[32m                $form->get('oldPassword')->addError(new FormError("Le mot de passe que vous avez tap√© n'est pas votre mot de passe actuel !"));[m
[32m+[m
[32m+[m[32m                } else {[m
[32m+[m[32m                   $newPassword =$passwordUpdate -> getNewPassword();[m
[32m+[m[32m                   $hash = $encoder->encodePassword($user, $newPassword);[m[41m  [m
[32m+[m[32m                   $user->setHash($hash);[m
[32m+[m
[32m+[m[32m                   $manager->persist($user);[m
[32m+[m[32m                   $manager-flush();[m
[32m+[m
[32m+[m[32m                   $this->addFlash([m
[32m+[m[32m                       'success' ,[m
[32m+[m[32m                       "Votre mot de passe a bien √©t√© modifi√© !"[m
[32m+[m[32m                   );[m
[32m+[m[32m                   return $this->redirectToRoute('homepage');[m
[32m+[m
[32m+[m[32m                }[m
[32m+[m[41m                [m
[32m+[m[41m                [m
[32m+[m
[32m+[m[32m            }[m
[32m+[m[32m            return $this->render('account/password.html.twig',[[m
[32m+[m[32m                'form' => $form->createView()[m
[32m+[m[32m            ]);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m            /**[m
[32m+[m[32m             * Permet d'afficher le profil de l'utilisateur connect√©[m
