[32m+[m[32m        return $this->url;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function setUrl(string $url): self[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->url = $url;[m
[32m+[m
[32m+[m[32m        return $this;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function getCaption(): ?string[m
[32m+[m[32m    {[m
[32m+[m[32m        return $this->caption;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function setCaption(string $caption): self[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->caption = $caption;[m
[32m+[m
[32m+[m[32m        return $this;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function getAd(): ?Ad[m
[32m+[m[32m    {[m
[32m+[m[32m        return $this->ad;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function setAd(?Ad $ad): self[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->ad = $ad;[m
[32m+[m
[32m+[m[32m        return $this;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
