[1mdiff --git a/config/bootstrap.php b/config/bootstrap.php[m
[1mnew file mode 100644[m
[1mindex 0000000..9c5a99c[m
[1m--- /dev/null[m
[1m+++ b/config/bootstrap.php[m
[36m@@ -0,0 +1,23 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32muse Symfony\Component\Dotenv\Dotenv;[m
[32m+[m
[32m+[m[32mrequire dirname(__DIR__).'/vendor/autoload.php';[m
[32m+[m
[32m+[m[32m// Load cached env vars if the .env.local.php file exists[m
[32m+[m[32m// Run "composer dump-env prod" to create it (requires symfony/flex >=1.2)[m
[32m+[m[32mif (is_array($env = @include dirname(__DIR__).'/.env.local.php')) {[m
[32m+[m[32m    foreach ($env as $k => $v) {[m
[32m+[m[32m        $_ENV[$k] = $_ENV[$k] ?? (isset($_SERVER[$k]) && 0 !== strpos($k, 'HTTP_') ? $_SERVER[$k] : $v);[m
[32m+[m[32m    }[m
[32m+[m[32m} elseif (!class_exists(Dotenv::class)) {[m
[32m+[m[32m    throw new RuntimeException('Please run "composer require symfony/dotenv" to load the ".env" files configuring the application.');[m
[32m+[m[32m} else {[m
[32m+[m[32m    // load all the .env files[m
[32m+[m[32m    (new Dotenv(false))->loadEnv(dirname(__DIR__).'/.env');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m$_SERVER += $_ENV;[m
[32m+[m[32m$_SERVER['APP_ENV'] = $_ENV['APP_ENV'] = ($_SERVER['APP_ENV'] ?? $_ENV['APP_ENV'] ?? null) ?: 'dev';[m
[32m+[m[32m$_SERVER['APP_DEBUG'] = $_SERVER['APP_DEBUG'] ?? $_ENV['APP_DEBUG'] ?? 'prod' !== $_SERVER['APP_ENV'];[m
[32m+[m[32m$_SERVER['APP_DEBUG'] = $_ENV['APP_DEBUG'] = (int) $_SERVER['APP_DEBUG'] || filter_var($_SERVER['APP_DEBUG'], FILTER_VALIDATE_BOOLEAN) ? '1' : '0';[m
