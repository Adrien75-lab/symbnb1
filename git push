[32m+[m[32m    {[m
[32m+[m[32m        if (!$this->users->contains($user)) {[m
[32m+[m[32m            $this->users[] = $user;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $this;[m
[32m+[m[32m    }[m
[32m+[m
