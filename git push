[32m+[m
[32m+[m[32m    public function addImage(Image $image): self[m
[32m+[m[32m    {[m
[32m+[m[32m        if (!$this->images->contains($image)) {[m
[32m+[m[32m            $this->images[] = $image;[m
[32m+[m[32m            $image->setAd($this);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $this;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function removeImage(Image $image): self[m
[32m+[m[32m    {[m
[32m+[m[32m        if ($this->images->contains($image)) {[m
[32m+[m[32m            $this->images->removeElement($image);[m
[32m+[m[32m            // set the owning side to null (unless already changed)[m
[32m+[m[32m            if ($image->getAd() === $this) {[m
[32m+[m[32m                $image->setAd(null);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $this;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
